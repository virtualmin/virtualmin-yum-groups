<comps>
  <group>
    <id>virtualmin-lamp</id>
    <default>false</default>
    <uservisible>true</uservisible>
    <display_order>1024</display_order>
    <name>Virtualmin LAMP Stack</name>
    <description>Virtualmin with BIND, Apache, MariaDB, PHP, Perl, Python, and a full mail stack</description>
    <packagelist>
      <!-- Mandatory LAMP packages -->
      <packagereq type="mandatory">httpd</packagereq>
      <packagereq type="mandatory">mod_ssl</packagereq>
      <!-- Default LAMP packages -->
      <packagereq type="default">bind</packagereq>
      <packagereq type="default">clamav-update</packagereq>
      <packagereq type="default">clamav</packagereq>
      <packagereq type="default">clamd</packagereq>
      <packagereq type="default">cyrus-sasl-gssapi</packagereq>
      <packagereq type="default">cyrus-sasl-plain</packagereq>
      <packagereq type="default">cyrus-sasl</packagereq>
      <packagereq type="default">dovecot</packagereq>
      <packagereq type="default">jailkit</packagereq>
      <packagereq type="default">milter-greylist</packagereq>
      <packagereq type="default">mod_fcgid</packagereq>
      <packagereq type="default">mod_http2</packagereq>
      <packagereq type="default">opendkim</packagereq>
      <packagereq type="default">php-imap</packagereq>
      <packagereq type="default">postfix</packagereq>
      <packagereq type="default">postgrey</packagereq>
      <packagereq type="default">procmail-wrapper</packagereq>
      <packagereq type="default">procmail</packagereq>
      <packagereq type="default">proftpd</packagereq>
      <packagereq type="default">spamassassin</packagereq>
      <packagereq type="default">wbm-jailkit</packagereq>
    </packagelist>
  </group>
